{
  "name": "idempotent-babel-polyfill",
  "version": "7.0.0",
  "description": "Import babel-polyfill multiple times",
  "homepage": "https://github.com/codejamninja/idempotent-babel-polyfill",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run test && babel src -d lib",
    "clean": "rimraf lib",
    "prepublish": "npm run build",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "format": "prettier --write ./src/**/*.js",
    "test": "npm run lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2"
  },
  "license": "MIT",
  "repository": "https://github.com/codejamninja/idempotent-babel-polyfill",
  "engines": {
    "node": ">= 4.0.0"
  },
  "eslintIgnore": [
    "lib"
  ]
}
